<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:System="clr-namespace:System;assembly=mscorlib">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="SharedStyles.xaml" />
        <ResourceDictionary Source="SystemControlStyles.xaml" />
        <ResourceDictionary Source="FavoritesBarStyle.xaml" />
        <ResourceDictionary Source="TabButtonGroupStyle.xaml" />
        <ResourceDictionary Source="FavoritesButtonStyle.xaml" />
        <ResourceDictionary Source="TabButtonStyle.xaml" />
        <ResourceDictionary Source="MainTabControlStyle.xaml" />
        <ResourceDictionary Source="TabControlStyle.xaml" />
        <ResourceDictionary Source="WindowStyle.xaml" />
        <ResourceDictionary Source="MenuStyle.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style TargetType="{x:Type Rectangle}"/>
</ResourceDictionary>
